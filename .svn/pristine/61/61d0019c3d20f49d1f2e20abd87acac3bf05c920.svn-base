package restaurant;


public class Restaurant {

	// Fields

	private Integer restaurantId;
	private String restaurantName;
	private Integer restaurantLovecount;
	private String restaurantType;
	private String restaurantInfo;
	

	// Constructors

	/** default constructor */
	public Restaurant() {
		
	}
	
	/** full constructor */
	public Restaurant(String restaurantName,
			Integer restaurantLovecount, String restaurantType,
			String restaurantInfo) {
		this.restaurantName = restaurantName;
		this.restaurantLovecount = restaurantLovecount;
		this.restaurantType = restaurantType;
		this.restaurantInfo = restaurantInfo;
		
	}

	// Property accessors

	public Integer getRestaurantId() {
		return this.restaurantId;
	}

	public void setRestaurantId(Integer restaurantId) {
		this.restaurantId = restaurantId;
	}

	public String getRestaurantName() {
		return this.restaurantName;
	}

	public void setRestaurantName(String restaurantName) {
		this.restaurantName = restaurantName;
	}

	public Integer getRestaurantLovecount() {
		return this.restaurantLovecount;
	}

	public void setRestaurantLovecount(Integer restaurantLovecount) {
		this.restaurantLovecount = restaurantLovecount;
	}

	public String getRestaurantType() {
		return this.restaurantType;
	}

	public void setRestaurantType(String restaurantType) {
		this.restaurantType = restaurantType;
	}

	public String getRestaurantInfo() {
		return this.restaurantInfo;
	}

	public void setRestaurantInfo(String restaurantInfo) {
		this.restaurantInfo = restaurantInfo;
	}

}
